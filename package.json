  "author": "getastrothemes",
  "license": "SEE LICENSE IN LICENSE.md",
  "packageManager": "pnpm@9.15.1",
  "private": true,
  "keywords": [
    "astro",
    "template",
    "tailwindcss"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/getastrothemes/folex-lite-astro.git"
  },
